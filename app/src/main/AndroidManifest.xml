<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.multithread.cocoon">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_cocoon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_cocoon"
        android:supportsRtl="true"
        android:usesCleartextTraffic="true"
        android:name=".CocoonApplication"
        android:theme="@style/Theme.Cocoon" >

        <activity android:name=".presentation.main.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".presentation.main.DetailActivity" />
    </application>

</manifest>